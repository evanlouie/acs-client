    "@types/react-dom": "^16.0.4",
    "electron": "^2.0.0-beta.7",
    "prettier": "^1.11.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
